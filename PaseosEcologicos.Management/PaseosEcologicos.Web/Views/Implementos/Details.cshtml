@model PaseosEcologicos.Model.Implementos

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Implementos</legend>

    <div class="display-label">ServicioId</div>
    <div class="display-field">@Model.ServicioId</div>

    <div class="display-label">Titulo</div>
    <div class="display-field">@Model.Titulo</div>

    <div class="display-label">Descripcion</div>
    <div class="display-field">@Model.Descripcion</div>

    <div class="display-label">Precio</div>
    <div class="display-field">@String.Format("{0:F}", Model.Precio)</div>

    <div class="display-label">Servicios</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Servicios).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


